{
  "name": "common",
  "targets": {
    "generate-tokens": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run generate:tokens",
        "cwd": "{workspaceRoot}"
      }
    },
    "get-version": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run update:version",
        "cwd": "{workspaceRoot}"
      }
    },
    "test": {
      "dependsOn": [
        "generate-tokens",
        "get-version"
      ],
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/common/jest.config.ts"
      }
    }
  }
}
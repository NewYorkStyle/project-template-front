{
  "name": "project-template-front",
  "version": "0.0.0",
  "license": "MIT",
  "author": "New York Style",
  "scripts": {
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "build:shell": "npm run generate:tokens && nx build shell",
    "build:authentication": "npm run generate:tokens && nx build authentication",
    "build:main": "npm run generate:tokens && nx build main",
    "build:user": "npm run generate:tokens && nx build user",
    "build:all": "npm run generate:tokens && nx run-many --target=build --projects=shell,authentication,main,user",
    "build:all:parallel": "npm run generate:tokens && nx run-many --target=build --projects=shell,authentication,main,user --parallel",
    "generate:tokens": "tsx tools/generators/generate-global-less.ts",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx pretty-quick --staged",
      "npx eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@types/js-cookie": "3.0.6",

    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/js-cookie": "3.0.6",
    "@types/lodash": "4.17.20",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/eslint-plugin": "8.46.0",

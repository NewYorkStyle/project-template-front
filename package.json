{
  "name": "project-template-front",
  "version": "2.5.1",
  "license": "MIT",
  "author": "New York Style",
  "type": "module",
  "scripts": {
    "dev": "npm run generate:tokens && webpack serve --mode development",
    "build": "npm run generate:tokens && webpack --mode production",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer dist",
    "prettier:check": "prettier --check \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "prettier:format": "prettier --write \"**/*.{ts,tsx,js,md,mdx,css,yaml}\"",
    "generate:tokens": "npx tsx tools/generators/generate-global-less.ts",
    "semantic-release": "semantic-release",
    "semantic-release:dry-run": "semantic-release --dry-run",
    "codestyle:check": "npm run prettier:check && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "build-storybook": "storybook build",
    "e2e": "npx playwright test",
    "e2e:changed": "node e2e/scripts/select-tests.mjs",
    "allure:show": "npx allure serve allure-results"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx pretty-quick --staged",
      "npx eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": true,
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "1.0.3",
    "antd": "5.27.4",
    "axios": "1.12.2",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "i18next-http-backend": "3.0.2",
    "js-cookie": "3.0.5",
    "lodash": "4.17.21",
    "mobx": "6.15.0",
    "mobx-react-lite": "4.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "6.0.0",
    "react-helmet": "6.1.0",
    "react-i18next": "16.0.0",
    "react-router": "7.9.4",
    "react-router-dom": "7.9.3"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@playwright/test": "1.56.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.6.1",
    "@semantic-release/changelog": "6.0.3",
